#hjson
// demo-158
// * 🎯配置启动demo-158
{
    description: "demo-158"
    // 转译器
    translators: "native"
    // 启动命令
    command: {
        // 命令：启动demo-158
        cmd: "./batch.exe"
        // 传入的命令参数
        cmdArgs: []
